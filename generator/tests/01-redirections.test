            shell.HandleError(err)
            return
        }
        stream3, err := streamManager.OpenStream(`file.txt`, runtime.STREAM_FLAG_APPEND)
        if err != nil {
            shell.HandleError(err)
            return
        }
        streamManager.Add(`4`, stream3, false)
        if err := streamManager.Close(`4`); err != nil {
            shell.HandleError(err)
            return
        }
  		if stream, err := streamManager.Get(`0`); err != nil {
